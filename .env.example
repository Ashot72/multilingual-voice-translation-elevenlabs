# ElevenLabs API Configuration
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Agent IDs for Voice Translation
# Get your agent IDs from: https://elevenlabs.io/app/agents
# AGENT_ID is used for "My Voice" / "Human Voice" option
AGENT_ID=your_agent_id_for_my_voice_here

# AGENT_ID2 is used for "Predefined Voice" option
AGENT_ID2=your_agent_id_for_predefined_voice_here
